{
  "name": "uor-content-management-client",
  "version": "0.1.0",
  "description": "A TypeScript client library for managing UOR Framework content",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src tests --ext .ts --max-warnings=9999",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "ts-node src/index.ts",
    "prepare": "npm run build",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "start:frontend": "cd frontend && npm run start"
  },
  "keywords": [
    "uor",
    "framework",
    "content",
    "management",
    "client"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.97",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "supertest": "^7.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "uuid": "^11.1.0"
  }
}
