{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": false,
    "autoJobCancelation": true
  },
  "env": {
    "NEXT_PUBLIC_MCP_API_URL": "/api/mcp"
  }
}
